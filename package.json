{
  "name": "movie-chooser",
  "version": "2.0.0",
  "scripts": {
    "preinstall": "bower install",
    "clean": "rm -rf client/dist",
    "babel": "babel client/ -d client/dist/ --minified --ignore client/dist/,client/bower_components/,client/lib",
    "build": "babel-node server/scripts/build.js",
    "deploy": "npm run clean && npm run babel && npm run build",
    "start": "npm run deploy && node index",
    "dev": "gulp dev"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "6.0.2",
    "del": "^2.2.2",
    "eslint": "2.9.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean-css": "^3.0.3",
    "gulp-nodemon": "^2.2.1",
    "gulp-sourcemaps": "^2.4.1",
    "nodemon": "^1.10.0",
    "pify": "^2.3.0",
    "shelljs": "^0.7.6",
    "spdy": "^3.4.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-node5": "^11.1.0",
    "babel-register": "^6.11.6",
    "body-parser": "^1.15.2",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "config": "^1.21.0",
    "express": "4.14.0",
    "mongodb": "^2.2.6",
    "serve-static": "1.11.1"
  }
}
